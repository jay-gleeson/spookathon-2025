{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "VERCEL_PYTHON_VERSION": "3.12"
    },
    "installCommand": "pip install -r requirements.txt --target=./.vercel/output/static/api"
  }
}